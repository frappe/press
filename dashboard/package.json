{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate-theme-config && vite",
    "build": "yarn generate-theme-config && vite build --base=/assets/press/dashboard/ && yarn copy-html-entry",
    "copy-html-entry": "cp ../press/public/dashboard/index.html ../press/www/dashboard.html",
    "generate-theme-config": "node ./generateThemeConfig.js",
    "lint": "eslint src"
  },
  "dependencies": {
    "@popperjs/core": "^2.0.6",
    "@stripe/stripe-js": "^1.3.0",
    "core-js": "^3.6.4",
    "feather-icons": "^4.26.0",
    "frappe-charts": "^1.3.0",
    "libarchive.js": "^1.3.0",
    "lodash": "^4.17.19",
    "luxon": "^1.22.0",
    "portal-vue": "^2.1.7",
    "register-service-worker": "^1.6.2",
    "socket.io-client": "^2.4.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "postcss": "^8",
    "postcss-easy-import": "^3.0.0",
    "prettier": "^1.19.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vite": "^2.2.3",
    "vite-plugin-vue2": "^1.4.4",
    "vue-template-compiler": "^2.6.11",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "yarn lint",
      "git add"
    ]
  }
}
